package de.hetzge.sgame.map;

import de.hetzge.sgame.common.IF_Module;
import de.hetzge.sgame.map.message.TileMapMessageHandler;
import de.hetzge.sgame.message.MessageConfig;

public class MapModule implements IF_Module {

	@Override
	public void init() {
		MessageConfig.INSTANCE.messageHandlerPool.registerMessageHandler(TileMapMessageHandler.class, new TileMapMessageHandler());
	}

	@Override
	public void update() {
	}

}
