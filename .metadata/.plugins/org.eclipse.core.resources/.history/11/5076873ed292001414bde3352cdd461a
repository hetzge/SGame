package de.hetzge.sgame.render;

public final class RenderUtil {

	private RenderUtil() {
	}

	public static <CONTEXT extends IF_RenderableContext> void render(CONTEXT context, IF_RenderInformation onScreen) {
		IF_RenderableWrapper<CONTEXT, ?> renderable = (IF_RenderableWrapper<CONTEXT, ?>) RenderConfig.INSTANCE.renderablePool.getRenderable(onScreen.getRenderableKey());
		renderable.render(context, onScreen);
	}

}
