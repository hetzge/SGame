package de.hetzge.sgame.network;

import org.nustaq.serialization.FSTObjectInput;
import org.nustaq.serialization.FSTObjectOutput;

public class InOutStreams {

	public final FSTObjectOutput fstObjectOutput;
	public final FSTObjectInput fstObjectInput;

	public InOutStreams(FSTObjectOutput fstObjectOutput, FSTObjectInput fstObjectInput) {
		this.fstObjectOutput = fstObjectOutput;
		this.fstObjectInput = fstObjectInput;
	}

}
