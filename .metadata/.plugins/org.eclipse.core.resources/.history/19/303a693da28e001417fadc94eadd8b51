package de.hetzge.sgame.network.message;

import java.util.LinkedList;
import java.util.List;

public class MessagePool {

	private final List<Object> buffer = new LinkedList<>();
	
	public List<Object> flush(){
		LinkedList<Object> result = new LinkedList<>(buffer);
		buffer.clear();
		return result;
	}
	
	
}
